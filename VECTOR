#include <iostream>
using namespace std;
#include <vector>
#include <string>

int main() {
  string k = "turpentine and turtles";
  vector<char> p = {'a','e','i','o','u'};
    vector<char> result;
    
    for (int i=0; i<k.size();i++){
        for(int j=0; j<p.size();j++){
            if (k[i]==p[j]){
                result.push_back(p[i]);
            }
        }
    }
    
    for (int i=0; i<result.size();i++){
        cout<<result[i];
    }
}
