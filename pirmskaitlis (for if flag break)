#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "ievadi naturālu skaitli.";
    cin>> n;


int flag=0;
    
    
for (int i=2; i<n;i++){
    if (n%i==0){
    flag=1;
    break;
    }
}
if (flag ==1){
    cout<<"nav pirmskaitlis";
}
else if (flag==0){
    cout<<"ir pirmskaitlis";
}

    return 0;
}
