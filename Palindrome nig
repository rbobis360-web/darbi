#include <iostream>
using namespace std;
#include <vector>

bool is_palindrome(string text){
    string reverse;
    for (int i=text.size()-1;i>=0;i--){
        reverse+=text[i];
    }
    if (text==reverse){
        return true;
    }
    else {
        return false;
    }
}
int main(){
    cout<<is_palindrome("racecar");
}
