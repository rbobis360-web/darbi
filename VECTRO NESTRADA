using namespace std;
#include<iostream>
#include <vector>

int main(){
int x;
int i;
int b;
vector<int> sk = {1,2,3,4,5};

cout<<"ko tu gribi ";
cin >> x;

for (int i = 0; i < sk.size()-1; i++){
    cout<<i;
    
    for (b=i+1;b < sk.size()-1;b++){
        if(b+i==x){
    cout<< b <<"+" <<i<<endl;
        }
    }
}

}
